# Exemplo 1: Definição de Classe e Métodos
# Tema 2 - Compiladores 2025.1

class Pessoa {
    var nome: string
    var idade: number

    void inicializar(n: string, i: number) {
        nome = n
        idade = i
    }

    string getNome() {
        return nome
    }
}

var p: Pessoa = new Pessoa()