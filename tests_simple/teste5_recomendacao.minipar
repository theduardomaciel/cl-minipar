# Programa de Teste 5: Recomendação simples (adaptado)
# Sem OO e sem dicionários; usa listas e filtros básicos

# Imprime itens do catálogo que não aparecem nos dois primeiros itens do histórico
func recomendar(list historico, list catalogo) -> void {
    println("Produtos recomendados para você:");

    for (string p in catalogo) {
        if (p == historico[0] || p == historico[1]) {
            # pula itens já vistos
        } else {
            println(p);
        }
    }
}

seq {
    list historico = ["Smartphone", "Jeans", "Micro-ondas", "Ficção"];
    list catalogo = [
        "Smartphone", "Laptop", "Tablet", "Fones de ouvido",
        "Camisa", "Jeans", "Jaqueta", "Sapatos",
        "Geladeira", "Micro-ondas", "Máquina de lavar", "Ar condicionado"
    ];

    recomendar(historico, catalogo);
}
